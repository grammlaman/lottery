
extends template/main
append vars
	-
		page_title = 'Главная страница'
		greensock = false

block content
	header#header
		.head
			a(href='#').head-logo
				+img('logo.png')
			a(href='#').head-name
				span Electronic Immigrant Investor Visa Lottery
			.page-select.head-language-select
				.page-select-header.head-button
					span Eng
					span.head-language-arrow
						span
						span
				.page-select-body
					a(href='#').head-button
						span Рус
			button.head-login.head-button#loginHead
				span Enter
				include ../img/lock.svg
			button.mob-none.part.button-blue.button-cabinet
				span Apply now
				span
		.head-underline
			div
	main.mainPage
		.youtube.display-none
			iframe(src="" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen)#iframe
			button.close-form
				svg(width="29" height="29" viewBox="0 0 29 29" fill="none" xmlns="http://www.w3.org/2000/svg")
					path(fill-rule="evenodd" clip-rule="evenodd" d="M8.91602 7.07076C8.40664 6.56138 7.58077 6.56138 7.07139 7.07076C6.56201 7.58014 6.56201 8.40601 7.07139 8.91539L12.2975 14.1415L7.07078 19.3683C6.5614 19.8777 6.5614 20.7035 7.07078 21.2129C7.58016 21.7223 8.40602 21.7223 8.9154 21.2129L14.1422 15.9862L19.3689 21.2129C19.8783 21.7223 20.7041 21.7223 21.2135 21.2129C21.7229 20.7035 21.7229 19.8777 21.2135 19.3683L15.9868 14.1415L21.2129 8.91542C21.7223 8.40604 21.7223 7.58017 21.2129 7.07079C20.7035 6.56141 19.8777 6.56141 19.3683 7.07079L14.1422 12.2969L8.91602 7.07076Z" fill="#C14747")
		.main
			.mainview
				.section
					.mainview-cont#first-view
						+img('logo.png').mainview-logo
						h1
							span This is the right time
							span to start your dream
						a(href='#second-view').mainview-scroll.mob-fixed#scroll-1
							span Scroll down
							+img('arr.svg')
						button.button-blue.part#participate
							span Apply now
							span
					.mainview-cont.mainview-second#second-view
						h2
							span You'll never be younger than
							span you are at this very moment.
						button.mainview-video
							span Watch the film
							+img('youtube-icon.svg')
						a(href='#third-view').mainview-scroll.mob-fixed.display-none#scroll-2
							span Scroll down
							+img('arr.svg')
			.main-anchor#third-view
		.program
			.section
				.program-cont
					.program-text
						p The Electronic Immigrant Investor Visa Lottery program registration period has no time restrictions and works by Sweepstakes principle. Each round designed for submission by 100,000 Participating Tickets*. No purchase necessary to enter or win. Most countries have variations in their laws and regulations regarding sweepstakes. You understand that it is your responsibility to ensure you are complying with the rules and regulations of your locality.
						br
						p Randomly selected Participating Ticket allows its holder to apply for Immigrant Investor Visa with investments not less than $900,000 (U.S.) provided by this program. Holder of randomly selected Participating Ticket who is not eligible for application and/or who wishes not to apply for Immigrant Investor Visa should receive $500,000 (U.S.) in compensation only.
					.program-steps
						h5 How to participate
						h6 Step 1
						span Fill up Entry Form
						h6 Step 2
						span Get Tickets
						h6 Step 3
						span Receive Results
						h6 Step 4
						span Apply for Investor Visa
				.underline
					div
		.count
			.section
				h3 The Final Countdown
				.count-row
					.count-cont
						span.count-span.count-random 000 000
						span.count-desc Winner Ticket
					.count-cont
						span.count-span
							span.count-span-hour 24
							span .
							span.count-span-minute 00
							span .
							span.count-span-second 00
						span.count-desc Time to Results
				.underline
					div
		.statistic
			.section
				.statistic-cont
					h2 Statistics
					.statistic-slider
						input(type='radio' name='statictic-slide' id='stat-slide1' style='display:none' checked)
						input(type='radio' name='statictic-slide' id='stat-slide2' style='display:none')
						input(type='radio' name='statictic-slide' id='stat-slide3' style='display:none')
						input(type='radio' name='statictic-slide' id='stat-slide4' style='display:none')
						.statistic-slider-cont
							.statistic-slide
								span 6
								span Round
							.statistic-slide-line
							.statistic-slide
								span 274
								span Entrants
							.statistic-slide-line
							.statistic-slide
								span 180
								span Participating Tickets
							.statistic-slide-line
							.statistic-slide
								span 12
								span.statistic-slide-i
									span i
								span Countries
						.statistic-slider-cont-label
							label(for='stat-slide1')
							label(for='stat-slide2')
							label(for='stat-slide3')
							label(for='stat-slide4')
